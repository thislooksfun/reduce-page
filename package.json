{
  "name": "reduce-page",
  "version": "0.3.0",
  "description": "Reduce a webpage to find the smallest test case",
  "bin": "dist/cli.js",
  "type": "module",
  "author": "thislooksfun <tlf@thislooks.fun> (https://thislooks.fun)",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prepare": "is-ci || husky",
    "dev": "tsx src/cli.ts",
    "build": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli wm",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all lint:check:*",
    "lint:check:eslint": "eslint \"**/*.ts\"",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix \"**/*.ts\"",
    "lint:fix:prettier": "prettier --write \"**/*\""
  },
  "dependencies": {
    "chalk": "5.6.0",
    "debug": "4.4.1",
    "express": "5.1.0",
    "livereload": "0.10.3",
    "parse5": "8.0.0",
    "postcss": "8.5.6",
    "purgecss": "7.0.2",
    "strip-bom": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.3",
    "@types/livereload": "0.9.5",
    "@types/node": "22.17.2",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "del-cli": "6.0.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-comment-length": "2.2.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "56.0.1",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "lint-staged": "16.1.5",
    "nock": "14.0.10",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  },
  "keywords": []
}
