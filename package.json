{
  "name": "reduce-page",
  "version": "0.4.2",
  "description": "Reduce a webpage to find the smallest test case",
  "bin": "dist/cli.js",
  "type": "module",
  "author": "thislooksfun <tlf@thislooks.fun> (https://thislooks.fun)",
  "license": "MIT",
  "repository": "https://github.com/thislooksfun/reduce-page",
  "bugs": "https://github.com/thislooksfun/reduce-page/issues",
  "homepage": "https://github.com/thislooksfun/reduce-page",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prepare": "is-ci || husky",
    "dev": "tsx src/cli.ts",
    "build": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli wm",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all lint:check:*",
    "lint:check:eslint": "eslint",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix",
    "lint:fix:prettier": "prettier --write \"**/*\""
  },
  "dependencies": {
    "chalk": "5.6.2",
    "debug": "4.4.3",
    "express": "5.2.1",
    "livereload": "0.10.3",
    "parse5": "8.0.0",
    "postcss": "8.5.6",
    "purgecss": "7.0.2",
    "strip-bom": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/js": "9.39.1",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.5",
    "@types/livereload": "0.9.5",
    "@types/node": "24.10.1",
    "del-cli": "7.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-comment-length": "2.2.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.2.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "lint-staged": "16.2.7",
    "nock": "14.0.10",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0"
  },
  "keywords": []
}
