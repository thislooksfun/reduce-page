{
  "name": "reduce-page",
  "version": "0.3.0",
  "description": "Reduce a webpage to find the smallest test case",
  "bin": "dist/cli.js",
  "type": "module",
  "author": "thislooksfun <tlf@thislooks.fun> (https://thislooks.fun)",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "dev": "ts-node src/cli.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli wm",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all lint:check:*",
    "lint:check:eslint": "eslint \"**/*.ts\"",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix \"**/*.ts\"",
    "lint:fix:prettier": "prettier --write \"**/*\""
  },
  "dependencies": {
    "chalk": "5.5.0",
    "debug": "4.4.1",
    "express": "4.21.2",
    "livereload": "0.10.3",
    "parse5": "7.3.0",
    "postcss": "8.5.6",
    "purgecss": "5.0.0",
    "strip-bom": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.23",
    "@types/jest": "29.5.14",
    "@types/livereload": "0.9.5",
    "@types/node": "22.17.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "del-cli": "5.1.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-comment-length": "0.9.3",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "56.0.1",
    "fast-check": "3.23.2",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.3.0",
    "nock": "13.5.6",
    "npm-run-all2": "5.0.0",
    "prettier": "3.6.2",
    "supports-color": "9.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "keywords": []
}
