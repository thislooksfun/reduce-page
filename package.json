{
  "name": "reduce-page",
  "version": "0.3.0",
  "description": "Reduce a webpage to find the smallest test case",
  "bin": "dist/cli.js",
  "type": "module",
  "author": "thislooksfun <tlf@thislooks.fun> (https://thislooks.fun)",
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",
    "dev": "ts-node src/cli.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli wm",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all lint:check:*",
    "lint:check:eslint": "eslint \"**/*.ts\"",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix \"**/*.ts\"",
    "lint:fix:prettier": "prettier --write \"**/*\""
  },
  "dependencies": {
    "chalk": "5.2.0",
    "debug": "4.3.4",
    "express": "4.18.2",
    "livereload": "0.9.3",
    "parse5": "7.1.2",
    "postcss": "8.4.39",
    "purgecss": "5.0.0",
    "strip-bom": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/livereload": "0.9.2",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "del-cli": "5.0.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-comment-length": "0.9.3",
    "eslint-plugin-deprecation": "1.4.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "47.0.0",
    "fast-check": "3.8.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.2.2",
    "nock": "13.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "supports-color": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "keywords": []
}
